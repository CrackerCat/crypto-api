{
  "name": "crypto-api",
  "description": "Hashing and encrypting library with no depedencies",
  "version": "0.5.1",
  "homepage": "https://github.com/nf404/crypto-api/",
  "author": {
    "name": "Aleksandr Mogilnikov",
    "email": "nf404.name@gmail.com",
    "url": "https://nf404.name/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nf404/crypto-api.git"
  },
  "bugs": {
    "url": "http://github.com/nf404/crypto-api/issues"
  },
  "main": "lib/crypto-api.js",
  "directories": {
    "test": "test",
    "lib": "lib",
    "example": "example"
  },
  "scripts": {
    "test": "mocha",
    "docs": "./node_modules/.bin/jsdoc2md lib/*.js",
    "jscoverage": "mocha --check-leaks -r jscoverage -R mocha-lcov-reporter > coverage/coverage.lcov",
    "istanbul": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --check-leaks -R spec",
    "coveralls": "cat coverage/coverage.lcov | ./node_modules/.bin/coveralls",
    "codacy": "cat coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "codeclimate": "cat coverage/coverage.lcov | ./node_modules/codeclimate-test-reporter/bin/codeclimate.js"
  },
  "dependencies": {
  },
  "devDependencies": {
    "jsdoc": ">=3.4.0",
    "jsdoc-to-markdown": ">=1.3.1",
    "inchjs": ">=0.4.0",
    "mocha": ">=2.3.4",
    "chai": ">=3.4.1",
    "jscoverage": ">=0.6.0",
    "istanbul": ">=0.4.1",
    "coveralls": ">=2.11.6",
    "codacy-coverage": ">=1.1.3",
    "mocha-lcov-reporter": ">=1.0.0",
    "codeclimate-test-reporter": ">=0.1.1"
  },
  "keywords": [
    "hash",
    "hashes",
    "md2",
    "md4",
    "md5",
    "sha0",
    "sha1",
    "sha224",
    "sha256"
  ]
}