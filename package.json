{
  "name": "crypto-api",
  "description": "Hashing and encrypting library with no depedencies",
  "version": "0.7.0",
  "homepage": "https://github.com/nf404/crypto-api/",
  "author": {
    "name": "Aleksandr Mogilnikov",
    "email": "nf404.name@gmail.com",
    "url": "https://nf404.name/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nf404/crypto-api.git"
  },
  "bugs": {
    "url": "http://github.com/nf404/crypto-api/issues"
  },
  "main": "lib/crypto-api.js",
  "directories": {
    "test": "test",
    "lib": "lib",
    "example": "example"
  },
  "scripts": {
    "docs": "jsdoc2md lib/*.js",
    "test": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly --dir coverage/cli -- --check-leaks -R spec && karma start karma.conf.js",
    "combine": "istanbul-combine -r lcovonly coverage/*/*.json",
    "coveralls": "coveralls < coverage/lcov.info",
    "codacy": "codacy-coverage < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "dependencies": {
  },
  "devDependencies": {
    "jsdoc": ">=3.4.3",
    "jsdoc-to-markdown": ">=1.3.9",
    "inchjs": ">=0.4.1",
    "mocha": ">=3.2.0",
    "chai": ">=3.5.0",
    "istanbul": ">=0.4.5",
    "istanbul-combine": ">=0.3.0",
    "karma": ">=1.3.0",
    "karma-coverage": ">=1.1.1",
    "karma-mocha": ">=1.3.0",
    "karma-mocha-reporter": ">=2.2.1",
    "karma-phantomjs-launcher": ">=1.0.2",
    "phantomjs-prebuilt": ">=2.1.14",
    "coveralls": ">=2.11.15",
    "codacy-coverage": ">=2.0.0",
    "codeclimate-test-reporter": ">=0.4.0"
  },
  "keywords": [
    "hash",
    "hashes",
    "md2",
    "md4",
    "md5",
    "sha0",
    "sha1",
    "sha224",
    "sha256",
    "sha384",
    "sha512",
    "hmac"
  ]
}