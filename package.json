{
  "name": "crypto-api",
  "description": "Hashing and encrypting library with no depedencies",
  "version": "0.7.5",
  "homepage": "https://github.com/nf404/crypto-api/",
  "author": {
    "name": "Aleksandr Mogilnikov",
    "email": "nf404.name@gmail.com",
    "url": "https://nf404.name/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nf404/crypto-api.git"
  },
  "bugs": {
    "url": "http://github.com/nf404/crypto-api/issues"
  },
  "main": "src/crypto-api.js",
  "directories": {
    "test": "test",
    "lib": "src",
    "example": "example"
  },
  "scripts": {
    "docs": "jsdoc2md lib/*.js",
    "test": "npm run test-istanbul && npm run test-karma",
    "test-istanbul": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly --dir coverage/istanbul -- --require babel-core/register --reporter spec --recursive test/test.js",
    "test-karma": "karma start",
    "combine": "istanbul-combine -r lcovonly coverage/*/*.json",
    "coveralls": "coveralls < coverage/lcov.info",
    "codacy": "codacy-coverage < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "build": "webpack --progress --hide-modules"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.3",
    "codeclimate-test-reporter": "^0.4.1",
    "coveralls": "^2.13.3",
    "inchjs": "^0.4.1",
    "istanbul": "^1.1.0-alpha.1",
    "istanbul-combine": "^0.3.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^1.3.9",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.3",
    "phantomjs-prebuilt": "^2.1.15",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "unminified-webpack-plugin": "^1.4.1",
    "webpack": "^3.6.0"
  },
  "keywords": [
    "hash",
    "hashes",
    "md2",
    "md4",
    "md5",
    "ripemd128",
    "ripemd160",
    "ripemd256",
    "ripemd320",
    "has160",
    "has-160",
    "sha0",
    "sha1",
    "sha224",
    "sha256",
    "sha384",
    "sha512",
    "sha512/224",
    "sha512/256",
    "whirlpool",
    "whirlpool-0",
    "whirlpool-t",
    "snefru",
    "hmac"
  ]
}
