{
  "name": "crypto-api",
  "description": "Hashing and encrypting library with no depedencies",
  "version": "0.5.1",
  "homepage": "https://github.com/nf404/crypto-api/",
  "author": {
    "name": "Aleksandr Mogilnikov",
    "email": "nf404.name@gmail.com",
    "url": "https://nf404.name/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nf404/crypto-api.git"
  },
  "bugs": {
    "url": "http://github.com/nf404/crypto-api/issues"
  },
  "main": "lib/crypto-api.js",
  "directories": {
    "test": "test",
    "lib": "lib",
    "example": "example"
  },
  "scripts": {
    "docs": "jsdoc2md lib/*.js",
    "test": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly --dir coverage/cli -- --check-leaks -R spec",
    "karma": "karma start karma.conf.js",
    "combine": "istanbul-combine -r lcovonly coverage/*/*.json",
    "coveralls": "coveralls < coverage/lcov.info",
    "codacy": "codacy-coverage < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "dependencies": {
  },
  "devDependencies": {
    "jsdoc": ">=3.4.0",
    "jsdoc-to-markdown": ">=1.3.1",
    "inchjs": ">=0.4.0",
    "mocha": ">=2.3.4",
    "chai": ">=3.4.1",
    "istanbul": ">=0.4.1",
    "karma": "*",
    "karma-coverage": "*",
    "karma-mocha": "*",
    "karma-mocha-reporter": "*",
    "karma-phantomjs-launcher": "*",
    "phantomjs": "*",
    "coveralls": ">=2.11.6",
    "codacy-coverage": ">=1.1.3",
    "codeclimate-test-reporter": ">=0.1.1"
  },
  "keywords": [
    "hash",
    "hashes",
    "md2",
    "md4",
    "md5",
    "sha0",
    "sha1",
    "sha224",
    "sha256"
  ]
}